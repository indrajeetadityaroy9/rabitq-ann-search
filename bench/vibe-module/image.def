Bootstrap: localimage
From: base.sif

%post
  apt-get update && apt-get install -y --no-install-recommends cmake g++ libomp-dev
  apt-get clean && rm -rf /var/lib/apt/lists/*

  pip install --no-cache-dir pybind11

  git clone https://github.com/YOUR_USERNAME/rabitq-ann-search /opt/cphnsw
  cd /opt/cphnsw && pip install --no-cache-dir .
